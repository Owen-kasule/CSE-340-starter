<main class="container">
  <h1><%= title %></h1>
  <% if (vehicles && vehicles.length) { %>
    <ul>
      <% vehicles.forEach(v => { %>
        <li>
          <a href="/inventory/detail/<%= v.inv_id %>">
            <%= v.inv_year %> <%= v.inv_make %> <%= v.inv_model %>
          </a>
        </li>
      <% }) %>
    </ul>
  <% } else { %>
    <p>No vehicles found for this classification.</p>
  <% } %>
</main>